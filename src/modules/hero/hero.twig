<link rel="stylesheet" href="/css/hero.css" type="text/css"><script> </script>
<link rel="stylesheet" href="/css/carousel.css" type="text/css"><script> </script>


<section class="Hero">

  <div class="Hero__wrapper">

    <div class="Carousel">

      <div class="Carousel__wrapper">

        <div class="Carousel__container  swiper-container">

          <div class="swiper-wrapper">

            <div class="Carousel__item  swiper-slide">
              <div class="media  media--landscape">
                <img src="/imgs/stack1.png" alt="">
              </div>
            </div>
            <div class="Carousel__item  swiper-slide">
              <div class="media  media--landscape">
                <img src="/imgs/stack2.png" alt="">
              </div>
            </div>
            <div class="Carousel__item  swiper-slide">
              <div class="media  media--landscape">
                <img src="/imgs/stack3.png" alt="">
              </div>
            </div>
            <div class="Carousel__item  swiper-slide">
              <div class="media  media--landscape">
                <img src="/imgs/stack4.png" alt="">
              </div>
            </div>
            <div class="Carousel__item  swiper-slide">
              <div class="media  media--landscape">
                <img src="/imgs/stack5.png" alt="">
              </div>
            </div>

          </div>

        </div>

        <div class="Carousel__pagination  swiper-container">
          <div class="swiper-wrapper">

            {% for i in 1..5 %}
              <div class="swiper-slide  Carousel__pagination__bullet"></div>
            {% endfor %}

          </div>
        </div>


      </div>

    </div>

  </div>

</section>
